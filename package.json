{
  "name": "@bigsaigon333/solve.io",
  "version": "0.0.4",
  "description": "알고리즘 문제 풀이를 위한 I/O CLI",
  "main": "dist/index.cjs",
  "files": [
    "/dist"
  ],
  "bin": {
    "solve.io": "dist/index.cjs"
  },
  "scripts": {
    "build": "tsup",
    "dev:watch": "tsx watch src/index.ts",
    "dev": "tsx src/index.ts",
    "eslint": "eslint --cache --fix .",
    "preinstall": "npx only-allow pnpm",
    "prepack": "pnpm build",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigsaigon333/solve.io.git"
  },
  "keywords": [
    "PS",
    "algorithms",
    "jest",
    "boj"
  ],
  "author": {
    "name": "Danny Kim",
    "email": "likepeppermint@gmail.com",
    "url": "https://bigsaigon333.github.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigsaigon333/solve.io/issues"
  },
  "homepage": "https://github.com/bigsaigon333/solve.io#readme",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/recommended": "^1.0.3",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "all-contributors-cli": "^6.26.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "only-allow": "^1.2.1",
    "prettier": "2.3.2",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "clipanion": "4.0.0-rc.2"
  },
  "engines": {
    "node": ">=16.13.1"
  },
  "packageManager": "pnpm@8.6.0"
}
